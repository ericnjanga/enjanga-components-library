{
  "name": "enjanga-components-library",
  "version": "1.0.6",
  "description": "Reusable component library for Next.js 13+ projects with Carbon design system.",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./*.css": "./dist/*.css"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn build",
    "build": "yarn build:css && tsup src/index.ts --dts --format esm,cjs --out-dir dist --external react --external react-dom --external @carbon/react --external clsx",
    "build:css": "sass --load-path=node_modules --no-source-map --style=compressed src/styles/index.scss dist/styles.css",
    "watch": "concurrently \"yarn watch:ts\" \"yarn watch:css\"",
    "watch:ts": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch --external react --external react-dom --external @carbon/react --external clsx",
    "watch:css": "sass -I node_modules --no-source-map --watch src/styles/index.scss dist/styles.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@carbon/react": "^1",
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@carbon/react": "^1.39.0",
    "@chromatic-com/storybook": "^4.0.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-mdx-gfm": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/types": "^8.6.14",
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/storybook__react": "^5.2.1",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^10.0.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "react": "^18.1.1",
    "react-docgen-typescript": "^2.4.0",
    "react-dom": "^18.1.1",
    "sass": "^1.92.1",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.14",
    "style-loader": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericnjanga/enjanga-components-library.git"
  },
  "bugs": {
    "url": "https://github.com/ericnjanga/enjanga-components-library/issues"
  },
  "homepage": "https://github.com/ericnjanga/enjanga-components-library#readme",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "enjanga-core-setup": "^1.0.14"
  }
}
